snippet rd "Disable rubocop rule" b
# rubocop:disable $1
# rubocop:enable $1
endsnippet

snippet rdm
# rubocop:disable Metrics/$1
# rubocop:enable Metrics/$1
endsnippet

snippet bb "byebug" b
byebug
endsnippet

snippet bs "byebug with sleep for Capybara" b
sleep 1; byebug
endsnippet

snippet ap "awesome print" b
ap '${1:var}': $1
endsnippet
