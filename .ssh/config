Host *
  SetEnv TERM=xterm-color

Host contabo
  User root
  HostName 144.91.84.227
  IdentityFile ~/.ssh/contabo_id_rsa

Host oracle
  User ubuntu
  IdentityFile ~/.ssh/oracle_id_rsa
  HostName 144.24.195.176

Host 192.168.2.1
    User root
    PubkeyAcceptedAlgorithms +ssh-rsa
    HostkeyAlgorithms +ssh-rsa

Host nuc
  HostName 192.168.2.15
  ForwardX11 yes
  SetEnv XAUTHORITY=/home/artem/.Xauthority
